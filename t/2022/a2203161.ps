.# =================================================================== #
.# Variable Includes
.# =================================================================== #
    INC VALID.RL
    INC UTIL_COMMON.CB
    INC CLI.RL

.# =================================================================== #
.# Scheduler Parameters
.# =================================================================== #

PARAM_LIST          LIST WITH NAMES
PARAM_TEST          CHAR    4
                    LISTEND

.# =================================================================== #
.# Equates for code clarity
.# =================================================================== #

V_FLAG              EQU     1   ...0 or 1
V_DATE              EQU     8   ...YYYYMMDD
MSG_HEIGHT          EQU     24
REFRESH_RATE        EQU     1000


.# =================================================================== #
.# Debugging Configuration
.# =================================================================== #

ENABLE_TEST         NUM     V_FLAG
TEST_CONFIG         LIST
                    LISTEND

.# =================================================================== #
.# File Declarations
.# =================================================================== #

OPTUM_FILE          FILE    VAR=60,TEXT
OPTUM_NAME          INIT    "/w/a2203161.txt"
OPTUM_LIST          LIST
OPTUM_ID            CHAR    4
OPTUM_AGY_NAME      CHAR    35
                    LISTEND

OPTUM_CSV_FILE      FILE    VAR=60,TEXT
OPTUM_CSV_NAME      INIT    "/w/a2203161.csv"
OPTUM_CSV_LIST      LIST
OPTUM_CSV_REC       CHAR    60
                    LISTEND

OPTUM_HEADER        LIST
OPTUM_HEADER_REC    INIT    "Agency ID":
                            "Agency Name"
                    LISTEND

OPTUM_R_ID          CHAR    1
CURRDATE            CHAR    V_DATE
START_DATETIME      CHAR    24
END_DATETIME        CHAR    24


.# =================================================================== #
.# Begin Program
.# =================================================================== #
.Manual and Autosys
    CALL RUN_NOW
    CHAIN C_NEXT    ...go to next program (usually the Menu)

.Impact Scheduler Hook
    INC SCHED_NP.SR     ...for jobs that don't use the print system
.    INC SCHED_WP.SR  ...for jobs requiring the print system


.#######################################################################
RUN_NOW ROUTINE
+ Configure runtime settings

.Save stack size at program start
    RETCOUNT ORIG_RETURN

.Set applicable traps
    TRAPSAVE TRAP_DATA
    TRAPCLR ALL
    TRAP QUIT NORESET IF F9

.Get run date
    CLOCK TIMESTAMP INTO CURRDATE

    MOVE "a2203161" TO C_PROG        ...Program name
    MOVE "Optum File" TO C_OPER  ...Program description
    MOVE "MENU" TO C_NEXT
    MOVE "RUN_NOW" TO C_LABEL
    MOVE BLANKS TO C_FILE
    MOVE BLANKS TO C_KEYS

.Check if the program was called from the command line
    IF (C_CMDPAR)
        CALL CMD_START USING C_PROG
    ELSE
        CALL X_BLANK_SCREEN
    ENDIF

.Configure test options based on file mode. Default is Production
    CALL UTIL_SET_TESTMODE USING PARAM_TEST:  ...scheduler param
                                 ENABLE_TEST:  ...test toggle
                                 TEST_CONFIG  ...test settings list

.Set up run mode or build SCH record
    CALL SCHED_PICK

    SWITCH RUN_MODE
        CASE "N" | "B"
            CALL MAIN_PROCESS
        DEFAULT
    ENDSWITCH

    CALL NORMAL_EOJ

    RETURN
    ENDROUTINE


.#######################################################################
MAIN_PROCESS LROUTINE
+ Core logic

    debug
    CALL INITIALIZE
    CALL BUILD_CLI_LIST
    CALL CREATE_CLI_LIST
    CALL CLEAN_UP

    RETURN
    ENDROUTINE


.#######################################################################
INITIALIZE LROUTINE
+ Setup runtime variables
    CLOCK TIMESTAMP TO CURRDATE
    CLOCK CALENDAR TO START_DATETIME

.Get the files and EDI locations from valid 101
    CALL UTIL_GET_SYS_DIRS_COMMON

.TODO: set counter desc
    CALL UTIL_INIT_COUNTERS USING "Agencies Found":
                                  "":
                                  "":
                                  "":
                                  "":
                                  ""

    RETURN
    ENDROUTINE

.#######################################################################
BUILD_CLI_LIST LROUTINE

    CALL UTIL_XMESSAGE USING "Building CLI Agency List",MSG_HEIGHT
    
    PACK CLI_LEV2 WITH "00",BLANKS
    PACK SCRATCH WITH "cli.dbf "," ",OPTUM_NAME," -t ":
                      " 09-13 ":
                      " -p14-18eq#"",CLI_LEV2,"#"": 
                      " 34-68 "
 
    REFORMAT SCRATCH
    IF (OVER)
        debug
        CALL UTIL_ERRORMSG USING "Reformat failed","BUILD_CLI_AGENCY_LIST"
    ENDIF

    PACK SCRATCH WITH OPTUM_NAME," ",OPTUM_NAME," 01-04 -t -u"
    SORT SCRATCH
    IF (OVER)
        debug
        CALL UTIL_ERRORMSG USING "Sort failed","BUILD_CLI_LIST"
    ENDIF

    RETURN
    ENDROUTINE

.#######################################################################
CREATE_CLI_LIST LROUTINE
+ Core logic

    MOVE OGB_SEQ1 TO OGB_SEQ
    CALL OPEN_OGBFILE USING OPTUM_FILE,OPTUM_NAME
    CALL PREP_OGBFILE USING OPTUM_CSV_FILE,OPTUM_CSV_NAME
    CALL WRITE_OGBREC USING OPTUM_CSV_FILE,OPTUM_CSV_NAME,OPTUM_HEADER

    LOOP
        CALL READ_OGBFILE USING OPTUM_FILE,OPTUM_NAME,OPTUM_LIST
        WHILE (RESULT = 1)

...CALL UTIL_XCOUNTER USING X_CNT1,10
       
        CALL UTIL_CLI_FIND USING OPTUM_ID

        CMATCH "R" IN OPTUM_ID
        CONTINUE IF EQUAL
        CONTINUE IF (OPTUM_ID = "5000") 
        CONTINUE IF (OPTUM_ID = "5002")
        CONTINUE IF (OPTUM_ID = "5005")
        CONTINUE IF (OPTUM_ID = "5006")
        CONTINUE IF (OPTUM_ID = "5007")
        CONTINUE IF (OPTUM_ID = "5100")
        CONTINUE IF (OPTUM_ID = "5200")
        CONTINUE IF (OPTUM_ID = "5300")
        CONTINUE IF (OPTUM_ID = "5400")

        CALL WRITE_TO_CSV

    REPEAT

    CALL CLOSE_OGBFILE USING OPTUM_FILE,OPTUM_NAME

    RETURN
    ENDROUTINE

.#######################################################################
WRITE_TO_CSV LROUTINE
    debug
    PACK OPTUM_CSV_REC WITH (CHOP OPTUM_ID),COMMA:
                            (CHOP OPTUM_AGY_NAME),COMMA,BLANKS

    CALL WRITE_OGBREC USING OPTUM_CSV_FILE,OPTUM_CSV_NAME,OPTUM_CSV_LIST

    RETURN
    ENDROUTINE


.#######################################################################
CLEAN_UP LROUTINE
+ Cleanup work files and handle output files
    CLOCK CALENDAR INTO END_DATETIME



    RETURN
    ENDROUTINE


