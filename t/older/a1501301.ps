            ENDIF
            IF (SQUEEZE MEME_NET <> "BLUE")
               CONTINUE
            ENDIF
            IF ((TODAY >= MEME_EFF) AND (TODAY <= W_TRM))
               SWITCH MEME_PRD 
                 CASE "PPO  "
