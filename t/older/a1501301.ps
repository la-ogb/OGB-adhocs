MATCH_FILES       
         LOOP
            IF ((CR_RESULT <> 1) AND (PR_RESULT <> 1))
               BREAK
            ELSE
              IF (CR_RESULT <> 1)
                 CALL ON_PR  
                 CALL READ_PR  
              ELSE
                IF (PR_RESULT <> 1)
                   CALL ON_CURR                
                   CALL READ_CURR
                ELSE
                  IF (CR_KEY < PR_KEY)
                     CALL ON_CURR    
                     CALL READ_CURR 
                  ELSE
                     IF (PR_KEY < CR_KEY)
                        CALL ON_PR        
                        CALL READ_PR 
                     ELSE
                        CALL ON_BOTH
                        CALL READ_CURR
                        CALL READ_PR  
                     ENDIF
                  ENDIF
                ENDIF
              ENDIF
            ENDIF
         REPEAT
    RETURN

