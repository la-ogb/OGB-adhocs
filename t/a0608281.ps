PROCESS_MEME
            MOVE OGB_SEQ1 TO OGB_SEQ
            LOOP
              CALL READ_OGBFILE USING MEM_WRK_FD,MEM_WRK_NME,MEM_WRK_LIST
                BREAK IF (RESULT <> 1)
                ADD 1 TO X_CNT1
                UNPACK X_CNT1 INTO CNT1_4,CNT1_3
