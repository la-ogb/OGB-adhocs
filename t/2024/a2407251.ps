.# =================================================================== #
.# Begin Program
.# =================================================================== #
.Manual and Autosys
    CALL RUN_NOW
    CHAIN C_NEXT    ...go to next program (usually the Menu)

.Impact Scheduler Hook
    INC SCHED_NP.SR     ...for jobs that don't use the print system
.    INC SCHED_WP.SR  ...for jobs requiring the print system


.#######################################################################
RUN_NOW ROUTINE

.Save stack size at program start
    RETCOUNT ORIG_RETURN

.Set applicable traps
    CALL UTIL_SET_NORMAL_TRAPS

.Get run date
    CLOCK TIMESTAMP INTO CURRDATE

    MOVE "a2407251" TO C_PROG
    MOVE "VOID EXTND & ONEXC" TO C_OPER
    MOVE "MENU" TO C_NEXT
    MOVE "RUN_NOW" TO C_LABEL
    MOVE BLANKS TO C_FILE
    MOVE BLANKS TO C_KEYS

.Check if the program was called from the command line
    IF (C_CMDPAR)
        CALL CMD_START USING C_PROG
    ELSE
        CALL X_BLANK_SCREEN
    ENDIF

.Configure test options based on file mode. Default is Production
    CALL UTIL_SET_TESTMODE USING PARAM_TEST:  ...scheduler param
                                 ENABLE_TEST:  ...test toggle
                                 TEST_CONFIG  ...test settings list

.Set up run mode or build SCH record
    CALL SCHED_PICK

    SWITCH RUN_MODE
        CASE "N" | "B"
            CALL MAIN_PROCESS
        DEFAULT
    ENDSWITCH

    CALL NORMAL_EOJ

    RETURN
    ENDROUTINE


.#######################################################################
MAIN_PROCESS LROUTINE
+ Core logic
    debug
    CALL INITIALIZE
    CALL CLIP_REFORMAT
    CALL PROCESS_CLIP_FILE
    CALL CLEAN_UP

.    CALL UTIL_XCOUNTER USING X_CNT1,REFRESH_RATE
.    CALL UTIL_XMESSAGE USING "Add Code Here",MSG_HEIGHT
.    CALL UTIL_ERRORMSG USING "Not implemented","MAIN_PROCESS"

    RETURN
    ENDROUTINE


.#######################################################################
INITIALIZE LROUTINE
+ Setup runtime variables
    CLOCK TIMESTAMP TO CURRDATE
    CLOCK CALENDAR TO START_DATETIME

.Get the files and EDI locations from valid 101
    CALL UTIL_GET_SYS_DIRS_COMMON

.TODO: set counter desc
    CALL UTIL_INIT_COUNTERS USING "Counter 1 - Not Used":
                                  "Counter 2 - Not Used"

    CALL SET_RUN_CONFIG

    RETURN
    ENDROUTINE

.#######################################################################
CLIP_REFORMAT LROUTINE
+ Sort CLIP records for Extnd, Onexc, and rehired retirees

    CALL UTIL_XMESSAGE USING "Building work file...",MSG_HEIGHT

    PACK SCRATCH USING CLIP_FNS," ",CLIP_REFORMAT_FILE," -t":
                        " -p0013-0017eq#"ONEXC#""

    CALL UTIL_DO_REFORMAT USING SCRATCH,"Finding ONEXC CLIPS"

    PACK SCRATCH USING CLIP_FNS," ",CLIP_REFORMAT_FILE," -t -a":
						" -p0013-0017eq#"EXTND#""

    CALL UTIL_DO_REFORMAT USING SCRATCH,"Finding EXTND CLIPS"


	PACK SCRATCH WITH CLIP_REFORMAT_FILE, " ", CLIP_REFORMAT_FILE," 1-12 -t"
	CALL UTIL_DO_SORT USING SCRATCH, "Sorting work file"

    DEBUG
    RETURN
    ENDROUTINE

.#######################################################################
SET_RUN_CONFIG LROUTINE
+ Configure any runtime settings here

    MOVE OGB_SEQ1 TO OGB_SEQ .default to sequential read

.TODO: delete if not needed

    RETURN
    ENDROUTINE


.#######################################################################
CLEAN_UP LROUTINE
+ Cleanup work files and handle output files
    CLOCK CALENDAR INTO END_DATETIME

.TODO: final clean up

    RETURN
    ENDROUTINE

.#######################################################################
PROCESS_CLIP_FILE LROUTINE

    CALL OPEN_OGBFILE USING CLIP_REFORMAT_FD,CLIP_REFORMAT_FILE

    CALL PREP_OGBFILE USING CLIP_CSV_FD,CLIP_CSV_FILE
    CALL WRITE_OGBREC_SEQ USING CLIP_CSV_FD,CLIP_CSV_FILE,CLIP_CSV_HEADER

    LOOP
        CALL READ_OGBFILE_SEQ USING CLIP_REFORMAT_FD,CLIP_REFORMAT_FILE,CLIP_LIST
        WHILE (RESULT = 1)

        CLEAR CLI_LIST
        PACK CLI_REC WITH CLI_LIST,BLANKS

        CALL UTIL_CLI_FIND USING CLIP_ID1
        CONTINUE IF (NOT EQUAL)
                IF (CLIP_RT = "EN" | CLIP_RT = "RN")
        debug
            PACK CSV_LEV2 WITH CLI_LEV2
            PACK CSV_ID1 WITH CLIP_ID1
            PACK CSV_ID2 WITH CLIP_ID2
            PACK CSV_NET WITH CLIP_NET
            PACK CSV_RT WITH CLIP_RT
            PACK CSV_EFF WITH CLIP_EFF
            PACK CSV_TRM WITH CLIP_TRM
            CALL CREATE_CLIP_CSV
       ENDIF
        IF (CLI_LEV2 = "92" | CLIP_TRM = "")
        debug
            PACK CSV_LEV2 WITH CLI_LEV2
            PACK CSV_ID1 WITH CLIP_ID1
            PACK CSV_ID2 WITH CLIP_ID2
            PACK CSV_NET WITH CLIP_NET
            PACK CSV_RT WITH CLIP_RT
            PACK CSV_EFF WITH CLIP_EFF
            PACK CSV_TRM WITH CLIP_TRM
            CALL CREATE_CLIP_CSV
        ENDIF




        debug

    REPEAT
        debug
    RETURN
    ENDROUTINE

.#######################################################################
CREATE_CLIP_CSV LROUTINE

    PACK CSV_REC WITH   (CSV_ID1),COMMA:
                        (CSV_ID2),COMMA:
                        (CSV_LEV1),COMMA:
                        (CSV_LEV2),COMMA:
.. ADD NEW VARIABLES
                        (CSV_NET),COMMA:
                        (CSV_RT),COMMA:
                        (CSV_EFF),COMMA:
                        (CSV_TRM),COMMA

    CALL WRITE_OGBREC_SEQ USING CLIP_CSV_FD,CLIP_CSV_FILE,CSV_LIST
    debug
    CALL UTIL_XCOUNTER USING X_CNT2,REFRESH_RATE

    RETURN
    ENDROUTINE

.#######################################################################
SEND_EMAIL LROUTINE
+ Email the report
REPORT@         CHAR    @
RECIPIENTS      CHAR    200
SUBJECT         CHAR    100
BODY            CHAR    1000
BODY_BASE       CHAR    1000
...


    CALL UTIL_SET_SUBJECT USING SCRATCH,ENABLE_TEST,SUBJECT

.    CALL UTIL_PACK_BODY_AND_COUNTERS USING BODY,BODY_BASE

        CALL UTIL_SET_RECIPIENTS USING RECIPIENTS,TEST_EMAIL_TO
        CALL UTIL_EMAIL_NEOJ USING  C_PROG:
                                    RECIPIENTS:
                                    SUBJECT:
                                    BODY:
                                    START_DATETIME:
                                    END_DATETIME
.    ENDIF

    RETURN
    ENDROUTINE

